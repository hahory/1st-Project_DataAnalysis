rm(list = ls())
dataSeoul <- read.csv("data_comb.csv")
dataSeoul <- dataSeoul[, -1]
head(dataSeoul)
dataSeoul2 <- read.csv("seoul_data_base_GY.csv")
dataSeoul2 <- dataSeoul2[, -c(1,3)]
head((dataSeoul2))
boxplot(dataSeoul$Police)
boxplot(dataSeoul$CCTV)
boxplot(dataSeoul$Onperson)
boxplot(dataSeoul$Pub)
boxplot(dataSeoul$Crime)
#Pub, Crime에서 이상치가 있음을 boxplot에서 확인(dataSeoul)
boxplot(dataSeoul2$Police_M)
boxplot(dataSeoul2$CCTV)
boxplot(dataSeoul2$Onperson)
boxplot(dataSeoul2$Pub)
boxplot(dataSeoul2$Crime)
#Police_M,Pub, Crime에서 이상치가 있음을 boxplot에서 확인(dataSeoul2)
